{
  "name": "@geut/openapi-box",
  "version": "1.0.6",
  "description": "Generate TypeBox schemas from your openapi.",
  "type": "module",
  "browser": "./dist/client.cjs",
  "main": "./dist/client.cjs",
  "bin": "./bin/cli.js",
  "exports": {
    ".": {
      "require": "./dist/client.cjs",
      "import": "./src/client.js"
    },
    "./writer": {
      "require": "./dist/writer.cjs",
      "import": "./dist/writer.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "dist",
    "bin"
  ],
  "scripts": {
    "start": "node index.js",
    "build": "tsup src/client.js src/writer.js --dts",
    "test": "node --test",
    "posttest": "npm run lint",
    "lint": "standard",
    "test:types": "tsc",
    "prepublishOnly": "npm test && npm run build"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "@sindresorhus/safe-stringify": "^1.0.0",
    "code-block-writer": "^12.0.0",
    "fast-querystring": "^1.1.2",
    "meow": "^12.1.1",
    "ora": "^7.0.1",
    "prettier": "^3.0.1",
    "undici": "^5.24.0"
  },
  "devDependencies": {
    "@fastify/swagger": "^8.8.0",
    "@sinclair/typebox": "^0.30.4",
    "@types/node": "^20.6.2",
    "eslint": "^8.46.0",
    "fastify": "^4.21.0",
    "openapi-types": "^12.1.3",
    "qs": "^6.11.2",
    "standard": "^17.1.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "standard": {
    "env": [
      "node",
      "browser"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geut/openapi-box.git"
  },
  "keywords": [
    "create",
    "geut",
    "module"
  ],
  "author": {
    "name": "GEUT",
    "email": "contact@geutstudio.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/geut/openapi-box/issues"
  },
  "homepage": "https://github.com/geut/openapi-box#readme",
  "publishConfig": {
    "access": "public"
  }
}
